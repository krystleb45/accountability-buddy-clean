{
  "name": "accountability-buddy-clean",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.0.6",
    "turbo": "^2.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "npm@10.9.2"
}