{
  "name": "accountability-buddy-clean",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint --",
    "format": "turbo run format --",
    "prepare": "husky",
    "generate-mongoose-types": "turbo run generate-types & turbo run generate-types-for-frontend"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.0.6",
    "prettier": "3.6.2",
    "turbo": "^2.5.5"
  },
  "lint-staged": {
    "!.husky/**": [
      "npm run lint --",
      "npm run format --"
    ]
  },
  "packageManager": "npm@10.9.2"
}
