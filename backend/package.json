    "date-fns": "^4.1.0",
    "dotenv-flow": "^4.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.0.2",
    "googleapis": "^144.0.0",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "i18next": "^23.15.1",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^2.3.2",
